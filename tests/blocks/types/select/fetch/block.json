{
  "$schema": "https://app.blockstudio.dev/schema",
  "name": "blockstudio/type-select-fetch",
  "title": "Native Select Fetch",
  "category": "blockstudio-test-native",
  "icon": "star-filled",
  "description": "Native Blockstudio Select Fetch block.",
  "keywords": [
    "select",
    "fetch",
    "native"
  ],
  "blockstudio": {
    "attributes": [
      {
        "id": "select",
        "type": "select",
        "label": "Select",
        "multiple": true,
        "populate": {
          "type": "fetch",
          "arguments": {
            "url": "https://fabrikat.local/streamline/wp-json/wp/v2/posts",
            "urlSearch": "https://fabrikat.local/streamline/wp-json/wp/v2/posts?search="
          },
          "returnFormat": {
            "value": "id",
            "label": "title.rendered"
          }
        }
      }
    ]
  }
}
