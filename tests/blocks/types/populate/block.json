{
  "$schema": "https://app.blockstudio.dev/schema",
  "name": "blockstudio/type-populate-function",
  "title": "Native Populate function",
  "category": "blockstudio-test-native",
  "icon": "star-filled",
  "description": "Native Blockstudio Populate function block.",
  "keywords": [
    "populate",
    "function",
    "native"
  ],
  "supports": {
    "anchor": true
  },
  "blockstudio": {
    "attributes": [
      {
        "id": "postTypes",
        "type": "checkbox",
        "label": "Post types",
        "populate": {
          "type": "function",
          "function": "get_post_types"
        },
        "returnFormat": "both"
      },
      {
        "id": "postTypesArguments",
        "type": "checkbox",
        "label": "Post types arguments",
        "populate": {
          "type": "function",
          "function": "get_post_types",
          "arguments": [[], "objects"],
          "returnFormat": {
            "value": "name",
            "label": "label"
          }
        }
      },
      {
        "id": "valueLabel",
        "type": "checkbox",
        "label": "Value Label",
        "populate": {
          "type": "function",
          "function": "blockstudioCustomSelect"
        }
      },
      {
        "id": "wrongFunction",
        "type": "checkbox",
        "label": "Wrong function",
        "populate": {
          "type": "function",
          "function": "cant_find_this_function"
        }
      }
    ]
  }
}
