{
  "$schema": "https://app.blockstudio.dev/schema",
  "name": "blockstudio/type-group",
  "title": "Native Group",
  "category": "blockstudio-test-native",
  "icon": "star-filled",
  "description": "Native Blockstudio Group block.",
  "keywords": ["group", "native"],
  "blockstudio": {
    "attributes": [
      {
        "type": "group",
        "title": "Group",
        "attributes": [
          {
            "id": "text",
            "type": "text",
            "label": "Text"
          },
          {
            "id": "toggle",
            "type": "toggle",
            "label": "Toggle"
          },
          {
            "id": "toggle2",
            "type": "toggle",
            "label": "Toggle 2",
            "conditions": [
              [
                {
                  "id": "toggle",
                  "operator": "==",
                  "value": true
                }
              ]
            ]
          }
        ]
      },
      {
        "id": "group",
        "type": "group",
        "title": "Group ID",
        "attributes": [
          {
            "id": "text",
            "type": "text",
            "label": "Text"
          },
          {
            "id": "toggle3",
            "type": "toggle",
            "label": "Toggle 3"
          },
          {
            "id": "toggle4",
            "type": "toggle",
            "label": "Toggle 4",
            "conditions": [
              [
                {
                  "id": "group_toggle3",
                  "operator": "==",
                  "value": true
                }
              ]
            ]
          }
        ]
      },
      {
        "id": "group",
        "type": "group",
        "style": {
          "display": "grid",
          "grid-template-columns": "1fr 1fr",
          "grid-gap": "16px"
        },
        "attributes": [
          {
            "id": "text1",
            "type": "text",
            "label": "Text",
            "switch": false
          },
          {
            "id": "text2",
            "type": "text",
            "label": "Text",
            "switch": false
          }
        ]
      }
    ]
  }
}
