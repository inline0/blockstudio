{
  "name": ["core/*"],
  "blockstudio": {
    "extend": true,
    "attributes": [
      {
        "type": "group",
        "title": "Addons all",
        "attributes": [
          {
            "id": "allClasses",
            "type": "classes",
            "label": "All Classes",
            "set": [
              {
                "attribute": "class",
                "value": "{attributes.allClasses}"
              }
            ]
          },
          {
            "id": "allAttributes",
            "type": "attributes",
            "label": "All attributes"
          },
          {
            "id": "allCss",
            "type": "code",
            "language": "css",
            "label": "All css"
          },
          {
            "id": "allClass",
            "type": "text",
            "label": "All class",
            "set": [
              {
                "attribute": "class",
                "value": "all-{attributes.allClass}"
              }
            ]
          },
          {
            "id": "allClass2",
            "type": "text",
            "label": "All class 2",
            "set": [
              {
                "attribute": "class",
                "value": "all-{attributes.allClass2}-2"
              }
            ]
          },
          {
            "id": "allClass3",
            "type": "toggle",
            "label": "All class 3",
            "set": [
              {
                "attribute": "class",
                "value": "all-toggle"
              }
            ]
          },
          {
            "id": "allClass4",
            "type": "checkbox",
            "label": "All class 4",
            "multiple": true,
            "stylisedUi": true,
            "options": [
              {
                "value": "option-1",
                "label": "Option 1"
              },
              {
                "value": "option-2",
                "label": "Option 2"
              }
            ],
            "set": [
              {
                "attribute": "class",
                "value": "all-checkbox-{attributes.allClass4}"
              }
            ]
          },
          {
            "id": "allData",
            "type": "text",
            "label": "All Data",
            "set": [
              {
                "attribute": "data-test-attribute",
                "value": "{attributes.allData}"
              }
            ]
          }
        ]
      }
    ]
  }
}
