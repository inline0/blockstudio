{
  "$schema": "https://blockstudio.dev/schema/block",
  "name": "blockstudio/type-repeater",
  "title": "Native Repeater Override",
  "blockstudio": {
    "override": true,
    "attributes": [
      {
        "id": "text",
        "type": "text",
        "default": "Override test",
        "label": "Override label"
      },
      {
        "id": "repeater",
        "type": "repeater",
        "label": "Override repeater",
        "attributes": [
          {
            "id": "color",
            "type": "color",
            "label": "Override color"
          },
          {
            "id": "repeater",
            "type": "repeater",
            "attributes": [
              {
                "id": "number",
                "type": "number",
                "label": "Override number",
                "default": 20
              }
            ]
          }
        ]
      }
    ]
  }
}
