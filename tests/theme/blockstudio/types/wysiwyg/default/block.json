{
  "$schema": "https://blockstudio.dev/schema/block",
  "name": "blockstudio/type-wysiwyg",
  "title": "Native WYSIWYG",
  "category": "blockstudio-test-native",
  "icon": "star-filled",
  "description": "Native Blockstudio WYSIWYG block.",
  "keywords": [
    "wysiwyg",
    "native"
  ],
  "blockstudio": {
    "attributes": [
      {
        "id": "wysiwyg",
        "type": "wysiwyg",
        "label": "WYSIWYG",
        "default": "Default text",
        "toolbar": {
          "tags": {
            "headings": [
              1,
              2,
              3,
              4,
              5,
              6
            ]
          },
          "formats": {
            "bold": true,
            "italic": true,
            "orderedList": true,
            "strikethrough": true,
            "code": true,
            "link": {
              "opensInNewTab": true,
              "attributes": {
                "rel": "nofollow"
              }
            },
            "underline": true,
            "unorderedList": true,
            "textAlign": {
              "alignments": [
                "left",
                "center",
                "right",
                "justify"
              ]
            }
          }
        }
      }
    ]
  }
}