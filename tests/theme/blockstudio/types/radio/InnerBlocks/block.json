                    "name": "core/column",
                    "innerBlocks": [
                      {
                        "name": "core/heading",
                        "attributes": {
                          "content": "This is the right Heading Level 2.",
                          "level": 2
                        }
                      },
                      {
                        "name": "core/paragraph",
                        "attributes": {
                          "content": "This is the right Paragraph Level 1."
                        }
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ],
