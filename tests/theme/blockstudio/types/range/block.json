{
  "$schema": "https://blockstudio.dev/schema/block",
  "name": "blockstudio/type-range",
  "title": "Native Range",
  "category": "blockstudio-test-native",
  "icon": "star-filled",
  "description": "Native Blockstudio Range block.",
  "keywords": [
    "range",
    "native"
  ],
  "blockstudio": {
    "attributes": [
      {
        "id": "range",
        "type": "range",
        "label": "Number",
        "default": 10
      },
      {
        "id": "rangeZero",
        "type": "range",
        "label": "Number",
        "default": 0
      },
      {
        "id": "textOnZero",
        "type": "text",
        "label": "Text on zero",
        "conditions": [
          [
            {
              "id": "rangeZero",
              "operator": "==",
              "value": 0
            }
          ]
        ]
      }
    ]
  }
}
