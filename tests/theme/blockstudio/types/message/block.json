{
  "$schema": "https://blockstudio.dev/schema/block",
  "name": "blockstudio/type-message",
  "title": "Native Message",
  "category": "blockstudio-test-native",
  "icon": "star-filled",
  "description": "Native Blockstudio Message block.",
  "keywords": [
    "message",
    "native"
  ],
  "blockstudio": {
    "attributes": [
      {
        "type": "message",
        "label": "Message",
        "value": "Block name is {block.name}! Text value is: <strong>{attributes.text}</strong>"
      },
      {
        "id": "text",
        "type": "text",
        "label": "Text"
      }
    ]
  }
}
