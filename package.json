{
  "name": "blockstudio",
  "version": "7.0.0",
  "description": "The block framework for WordPress",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "wp-scripts build src/blocks/index.tsx --output-path=includes/admin/assets/blocks --config webpack.config.cjs",
    "start": "wp-scripts start src/blocks/index.tsx --output-path=includes/admin/assets/blocks --config webpack.config.cjs",
    "types": "quicktype includes/schemas/block.json -s schema -o src/types/block.ts && quicktype includes/schemas/blockstudio.json -s schema -o src/types/blockstudio.ts",
    "lint": "eslint src",
    "format": "prettier --write src",
    "check": "eslint src && tsc --noEmit",
    "fix": "eslint --fix src && prettier --write src",
    "playground": "tsx tests/unit/playground-server.ts",
    "playground:v7": "tsx tests/unit/playground-server-v7.ts",
    "test": "playwright test",
    "test:v7": "PLAYGROUND_PORT=9401 playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "wp-env:start": "wp-env start && curl -s -X POST http://localhost:8888/wp-json/blockstudio-test/v1/e2e/setup",
    "wp-env:stop": "wp-env stop",
    "wp-env:reset": "wp-env stop; wp-env clean all; wp-env start && wp-env run cli wp theme activate theme && wp-env run cli wp rewrite structure '/%postname%/' --hard && curl -s -X POST http://localhost:8888/wp-json/blockstudio-test/v1/e2e/setup",
    "test:e2e": "playwright test --config=playwright.wp-env.config.ts; npm run wp-env:reset",
    "test:e2e:headed": "playwright test --config=playwright.wp-env.config.ts --headed; npm run wp-env:reset",
    "test:e2e:debug": "playwright test --config=playwright.wp-env.config.ts --debug"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.20.0",
    "@codemirror/lang-css": "^6.2.1",
    "@codemirror/lang-html": "^6.4.8",
    "@codemirror/lang-javascript": "^6.2.1",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-php": "^6.0.1",
    "@emotion/react": "^11.10.5",
    "@floating-ui/dom": "^1.7.3",
    "@hello-pangea/dnd": "^18.0.1",
    "@tiptap/extension-link": "^3.2.0",
    "@tiptap/extension-text-align": "^3.2.0",
    "@tiptap/extension-underline": "^3.2.0",
    "@tiptap/react": "^3.2.0",
    "@tiptap/starter-kit": "^3.2.0",
    "@uiw/codemirror-theme-github": "^4.21.21",
    "@uiw/react-codemirror": "^4.21.21",
    "@wordpress/api-fetch": "^7.22.0",
    "@wordpress/block-editor": "^15.1.0",
    "@wordpress/blocks": "^15.1.0",
    "@wordpress/components": "^32.1.0",
    "@wordpress/compose": "^7.39.0",
    "@wordpress/data": "^10.22.0",
    "@wordpress/dom-ready": "^4.22.0",
    "@wordpress/element": "^6.22.0",
    "@wordpress/hooks": "^4.22.0",
    "@wordpress/i18n": "^6.12.0",
    "@wordpress/icons": "^11.6.0",
    "@wordpress/interactivity": "^6.22.0",
    "@wordpress/plugins": "^7.22.0",
    "html-react-parser": "^5.0.6",
    "htmlparser2": "^10.1.0",
    "lodash-es": "^4.17.21",
    "node-html-parser": "^7.0.1",
    "parse5": "^8.0.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^6.0.0",
    "react-router-dom": "^7.13.0",
    "style-to-object": "^1.0.4",
    "usehooks-ts": "^3.1.0"
  },
  "overrides": {
